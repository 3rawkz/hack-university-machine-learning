    "import random as rm\n",
    "import matplotlib.pylab as plt\n",
    "drift = 0.0\n",
    "L=[-2,-1,1,2];\n",
    "L=[-1,1]\n",
    "def Random_Walk(x, L, drift, n):\n",
    "    a=[]   \n",
    "    for i in range(n):\n",
    "        x = x + rm.choice(L) + drift \n",
    "        a.append(x)\n",
    "    return(a)\n",
    "        \n",
    "plt.plot(Random_Walk(5, L,drift,5000))"
   ]
